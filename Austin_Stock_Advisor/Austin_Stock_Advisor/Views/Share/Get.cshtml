@model IEnumerable<AustinStockAdvisor.Library.Share>

@{
    ViewData["Title"] = "Get";
}
                      <head>
                          <link href="@Url.Content("~/bootstrap-4.0.0-dist/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
                      </head>                      

<table class="table">
    <thead>
        <tr>
            <th scope="col">
                Index
            </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Date)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.WeekDay)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Symbol)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Open)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.High)
                </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Low)
            </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Close)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Volume)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.Change)
                </th>
                <th scope="col">
                    @Html.DisplayNameFor(model => model.ChangePercent)
                </th>
        </tr>
    </thead>
    <tbody>
@{
        var index = 0;
        foreach (var item in Model) {
        ++index;
        <tr>
            <th scope="row">@index</th>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WeekDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Symbol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Open)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.High)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Low)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Close)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Volume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Change)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChangePercent)
            </td>
        </tr>
}
        }
    </tbody>
</table>
