@model AustinsFirstProject.CoreLibrary.Database.Shares

@{
    ViewData["Title"] = "Index";
}


<h2>@Model.Identity</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Shares</th>
        </tr>
    </thead>
    <tbody>        

    @foreach (var item in Model._shares) {
        <tr>
            <td>
                @Html.ActionLink(@item.Ticker, "Ticker", new { ticker = item.Ticker })
            </td>
            <td>
            </td>
        </tr>
}
    </tbody>
</table>


